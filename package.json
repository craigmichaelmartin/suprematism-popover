{
  "name": "suprematism-popover",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "main": "out-tsc/index.js",
  "typings": "out-tsc/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:css && npm run build:copyAssets",
    "build:copyAssets": "cp -R src/*.css ./out-tsc && cp -R src/*html ./out-tsc",
    "build:css": "node-sass src/popover.component.scss src/popover.component.css",
    "build:tsc": "tsc -p src",
    "deploy": "npm run build && ng github-pages:deploy",
    "e2e": "protractor",
    "example": "ng serve",
    "lint": "npm run lint:src && npm run lint:example",
    "lint:src": "tslint \"src/**/*.ts\"",
    "lint:example": "tslint \"example/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "prepublish": "npm run build",
    "postpublish": "rm -rf out-tsc && rm -rf src/popover.component.css",
    "test": "ng test"
  },
  "private": true,
  "peerDependencies": {
    "@angular/core": "^2.0.0"
  },
  "devDependencies": {
    "@angular/core": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@types/jasmine": "^2.2.30",
    "angular-cli": "1.0.0-beta.16",
    "bootstrap": "4.0.0-alpha.4",
    "codelyzer": "~0.0.26",
    "core-js": "^2.4.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2",
    "zone.js": "^0.6.23"
  }
}
